            "enterMode": "div",
            "tags": [
              "strong",
              "em",
              "u",
              "a",
              "ul",
              "ol",
              "h2",
              "h3",
              "hr",
              "pre",
              "code"
            ],
            "default": "<p style=\"text-align: center;\"></p>"
          },
          {
            "name": "endScreenMediumNoSuccess",
            "label": "End screen medium (user not successful)",
            "type": "library",
